{
  "name": "sgvsv/php-yandex-zen-rss-feed",
  "description": "Generates RSS feed in Yandex Zen supported format",
  "type": "library",
  "keywords": [
    "rss",
    "yandex",
    "generator",
    "zen"
  ],
  "homepage": "https://github.com/sgvsv/php-yandex-zen-rss-feed",
  "support": {
    "issues": "https://github.com/sgvsv/php-yandex-zen-rss-feed/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Vasily Svitkin",
      "email": "svitkin@gmail.com",
      "role": "lead"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "sgvsv\\Yandex\\Zen\\": "src/"
    }
  },
  "require": {
    "php": ">=7.2",
    "ext-fileinfo": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  }
}
